{
    "name": "wpify/clean-uploads",
    "type": "wp-cli-package",
    "description": "",
    "homepage": "https://gitlab.com/wpify/clean-uploads",
    "license": "MIT",
    "authors": [],
    "require": {
        "wp-cli/wp-cli": "^2.5"
    },
    "require-dev": {
        "wp-cli/wp-cli-tests": "^3.0.11"
    },
    "config": {
        "process-timeout": 7200,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "bundled": false,
        "commands": [
            "clean-uploads"
        ]
    },
    "autoload": {
        "psr-4": {
            "WP_CLI\\Wpify\\": "src/"
        },
        "files": [
            "clean-uploads-command.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://gitlab.com/wpify/clean-uploads/issues"
    }
}
